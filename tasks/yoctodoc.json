{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "applicationName": "Yoctodoc plugin",
    "linenums": true,
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}