{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "linenums": true,
    "collapseSymbols": false
  },
  "opts": {
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/postman-jsdoc-theme",
    "footer": "Documentation for application <b>%appname%<b> was generated at %date% with <a href='https://www.npmjs.com/package/yoctodoc'>yocto-doc</a> module - Copyright Yocto Â© %year%"
  }
}